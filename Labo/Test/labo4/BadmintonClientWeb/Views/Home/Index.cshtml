@model IEnumerable<BadmintonClientWeb.ServiceReference1.SportClub>

@{
    ViewBag.Title = "Index";
}

<h2>Sportclubs</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Naam)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Naam)
        </td>
        <td>
            @Html.ActionLink("Leden", "GetLeden", new { id=item.ID })
        </td>
    </tr>
}

</table>
